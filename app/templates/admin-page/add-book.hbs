<BackButton />

<AddForm @heading="Add Book">
  <FormComponent @btn-value="Add Book" @id="add-form" as |errorMsg f|>
    <BookForm
      @errorMsg={{errorMsg}}
      @validateIsEmpty={{f.validateIsEmpty}}
      @validateUserName={{f.validateUserName}}
      @validateBookTitle={{f.validateBookTitle}}
    />
  </FormComponent>
</AddForm>